#include<stdio.h>
int main()
{
    int n;
    printf("enter n valiue\n");
    scanf("%d",&n);
    int i,j,k;
        for(i=1;i<=n;i++)
        {
            for(k=1;k<n-i;k++)
            {
                printf(" ");
            }
            for(j=1;j<=n;j++)
            {
                printf("*");
            }
            printf("\n");
    }
}
