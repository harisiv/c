#include<stdio.h>
int main()
{
int n,i,j,start=0,end=size-1;
int size=(2*n)-1;
char *name =  "SIVA";
int temp=0;
printf("enter n value");
scanf("%d",&n);
while(n>0)
{
for(i=start;i<=end;i++)
{
for( j=start;j<=end;j++)
{
if(i==start||i==end||j==start||j==end)
{
a[i][j]=name[temp];
}
}
temp++;
start++;
end--;
n--;
}
for(i=0;i<size;i++)
{
for(j=0;j<size;j++)
{
printf("%c",a[i][j]);
}
printf("\n");
}
}
